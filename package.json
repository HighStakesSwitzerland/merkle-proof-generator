    "@types/crypto-js": "^4.1.1",
    "ethereumjs-util": "^7.1.5",
    "merkletreejs": "^0.2.32",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@types/node": "^18.7.21",
    "globby": "^13.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
